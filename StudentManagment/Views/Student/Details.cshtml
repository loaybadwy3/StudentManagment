@model StudentManagment.Models.Student

<form method="post" asp-action="Details">
    <input asp-for="Id" hidden/>
  <div class="form-group">
    <label asp-for="StudentName"></label>
    <input asp-for="StudentName" class="form-control" disabled>
  </div>
  <div class="form-group">
    <label asp-for="StudentNumber"></label>
    <input asp-for="StudentNumber" class="form-control" disabled>
  </div>
  <div class="form-group">
    <label asp-for="StudentCourses"></label>
    <input asp-for="StudentCourses" class="form-control" disabled>
  </div>
  <div class="row justify-content-center">
    <a asp-controller="Student" asp-action="Index" class="btn btn-primary col-5 m-1">Back</a>
   </div>
   <a asp-controller="Student" asp-action="Update" asp-route-id="@Model.Id" class="btn btn-primary m-1">Update</a>
    <a asp-controller="Student" asp-action="DeleteGet" asp-route-id="@Model.Id" class="btn btn-danger m-1 ">Delete</a>
</form>


